<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:controls="using:RGR.Views.Elements">
  <Design.PreviewWith>
    <controls:Element_Or />
  </Design.PreviewWith>

  <Style Selector="controls|Element_Or">
    <!-- Set Defaults -->
    <Setter Property="Template">
      <ControlTemplate>
        <Canvas Width="65" Height="50" Background="Transparent">
          <Ellipse Name="in0" Fill="Gray"  Stroke="Black" StrokeThickness="1" Height="12" Width="12" Canvas.Top="4" Canvas.Left="2"/>
          <Ellipse Name="in1" Fill="Gray"  Stroke="Black" StrokeThickness="1" Height="12" Width="12" Canvas.Top="25" Canvas.Left="2"/>
          <Path Fill="LightGray" Data="m 8,9 c 10,10 10,15 0,26 h 15 c 0,0 35,-14 2,-26 Z" StrokeThickness="2" Stroke="Black" Canvas.Left="5" Canvas.Top="-1"/>
          <Ellipse Name="out0" Fill="Gray"  Stroke="Black" StrokeThickness="1" Height="12" Width="12" Canvas.Top="14" Canvas.Left="43"/>
        </Canvas>
      </ControlTemplate>
    </Setter>
  </Style>
</Styles>
