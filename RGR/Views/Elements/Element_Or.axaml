<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:controls="using:RGR.Views.Elements">
  <Design.PreviewWith>
    <controls:Element_Or />
  </Design.PreviewWith>

  <Style Selector="controls|Element_Or">
    <!-- Set Defaults -->
    <Setter Property="Template">
      <ControlTemplate>
        <Canvas Width="45" Height="35" Background="Transparent">
          <Ellipse Name="in0" Fill="Gray"  Stroke="Black" StrokeThickness="1" Height="8" Width="8" Canvas.Top="4" Canvas.Left="8"/>
          <Ellipse Name="in1" Fill="Gray"  Stroke="Black" StrokeThickness="1" Height="8" Width="8" Canvas.Top="21" Canvas.Left="8"/>
          <Path Fill="LightGray" Data="m 5,5 c 7,3 20,15 0,26 h 15 c 0,0 30,-13 0,-26 Z" StrokeThickness="1" Stroke="Black" Canvas.Left="5" Canvas.Top="-1"/>
          <Ellipse Name="out0" Fill="Gray"  Stroke="Black" StrokeThickness="1" Height="7" Width="7" Canvas.Top="14" Canvas.Left="38"/>
        </Canvas>
      </ControlTemplate>
    </Setter>
  </Style>
</Styles>
