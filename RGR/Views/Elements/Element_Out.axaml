<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:controls="using:RGR.Views.Elements">
  <Design.PreviewWith>
    <controls:Element_Out />
  </Design.PreviewWith>

  <Style Selector="controls|Element_Out">
    <!-- Set Defaults -->
    <Setter Property="Template">
      <ControlTemplate>
        <Canvas Width="40" Height="56" Background="Transparent">
          <Rectangle Width="30" Height="50" Canvas.Left="5" Canvas.Top="3" Fill="Transparent"/>
          <Border BorderBrush="Black" BorderThickness="1" Width="30" Height="50" Canvas.Left="5" Canvas.Top="3">
            <TextBlock Text="{TemplateBinding status}">
            </TextBlock>
          </Border>
        </Canvas>
      </ControlTemplate>
    </Setter>
  </Style>
</Styles>
