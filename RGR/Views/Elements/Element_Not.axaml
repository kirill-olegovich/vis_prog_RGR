<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:controls="using:RGR.Views.Elements">
  <Design.PreviewWith>
    <controls:Element_Not />
  </Design.PreviewWith>

  <Style Selector="controls|Element_Not">
    <!-- Set Defaults -->
    <Setter Property="Template">
      <ControlTemplate>
        <Canvas Width="46" Height="30" Background="Transparent">
          <Ellipse Name="in0" Fill="Gray" Stroke="Black" StrokeThickness="1" Height="10" Width="10" Canvas.Top="9" Canvas.Left="0"/>
          <Polygon Points="7,2 7,23 30,12.5 7,2" Fill="LightGray" Stroke="Black" StrokeThickness="1" Canvas.Left="3"/>
          <Ellipse Name="out0" Fill="Gray" Stroke="Black" StrokeThickness="1" Height="10" Width="10" Canvas.Top="9" Canvas.Left="30"/>
        </Canvas>
      </ControlTemplate>
    </Setter>
  </Style>
</Styles>
